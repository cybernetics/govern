{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Jest All / govern",
        "program": "${workspaceRoot}/node_modules/jest/bin/jest",
        "cwd": "${workspaceRoot}/packages/govern",
        "args": ["--runInBand"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Jest Current File / govern",
        "program": "${workspaceRoot}/node_modules/jest/bin/jest",
        "cwd": "${workspaceRoot}/packages/govern",
        "args": ["${file}"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }